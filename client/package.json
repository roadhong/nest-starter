{
  "name": "nest-starter-client",
  "appName": "Nest Starter Client",
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "clear": "rimraf dist",
    "start": "vite",
    "build": "pnpm run clear && vite build",
    "build:docker": "pnpm run clear && vite build --mode docker",
    "build:swagger": "rimraf swagger && cross-env zone=local build_type=swagger vite build",
    "build:management": "rimraf management && cross-env zone=local build_type=management vite build",
    "preview": "vite preview",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:check": "eslint './src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "@codemirror/lint": "^6.9.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource/barlow": "^5.2.8",
    "@fontsource/roboto": "^5.2.9",
    "@iconify/react": "^6.0.2",
    "@mui/icons-material": "^7.3.7",
    "@mui/lab": "7.0.1-beta.21",
    "@mui/material": "^7.3.7",
    "@mui/x-data-grid": "^8.23.0",
    "@react-oauth/google": "^0.13.4",
    "@uiw/react-codemirror": "^4.25.4",
    "apexcharts": "^5.3.6",
    "axios": "^1.13.2",
    "codemirror-json5": "^1.0.3",
    "dayjs": "^1.11.19",
    "es-toolkit": "^1.43.0",
    "json5": "^2.2.3",
    "localforage": "^1.10.0",
    "minimal-shared": "^1.1.5",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "mobx-react-lite": "^4.1.1",
    "nestjs-api-axios": "file:modules/nestjs-api-axios",
    "notistack": "^3.0.2",
    "qs": "^6.14.1",
    "react": "^19.2.3",
    "react-apexcharts": "^1.9.0",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0",
    "react-router-dom": "^7.12.0",
    "react-split": "^2.0.14",
    "simplebar-react": "^3.3.2",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/figlet": "^1.7.0",
    "@types/node": "^25.0.3",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.23",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "figlet": "^1.9.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "5.9.3",
    "vite": "^7.3.1"
  }
}
