{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "┏━━━━━━━━━━━ Nest Client ━━━━━━━━━━━┓"
    },
    {
      "type": "node",
      "name": "┃Management Run",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "start"],
      "skipFiles": ["<node_internals>/**"],
      "env": {
        "zone": "local",
        "port": "3000"
      },
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "┃Management For Debug",
      "url": "http://localhost:3000/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "┗━━━━━━━━━━━ Nest Client ━━━━━━━━━━━┛"
    }
  ]
}
